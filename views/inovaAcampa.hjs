    padding: 10px; 
}
#description-text h3{
    font-family: customFont;
    font-weight: 300;
    font-size: 3em;
    border-color: white;
    border-style: solid;
    border-width: thin;
    /*text-transform: none;*/
    background: rgb(0, 0, 0); /* fallback color */
    background: rgba(0, 0, 0, 0.7);
    padding: 10px; 
}

.legend span{

        /*font-family: 'Gotham SSm A', 'Gotham SSm B',"Helvetica Neue", Helvetica, Arial, sans-serif;*/
