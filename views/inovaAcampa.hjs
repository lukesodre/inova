.legend {
    /*opacity: 0.5;*/
    top:45%;
    /*left: 1%;*/
    color: white;
    /*position: absolute;*/
        left: 0;
    position:absolute;
    text-align:center;
    width: 100%


}
@font-face {
    font-family: customFont;
    src: url(font/font2.ttf);
}

h2,h3,h4,p,
.section-heading{
    font-family: customFont;

    
}

p{text-align: justify;}

#description-text h4{
    font-family: customFont;
    font-weight: 300;
    font-size: 2em;

    border-color: white;
    border-style: solid;
    border-width: thin;
    /*text-transform: none;*/
    background: rgb(0, 0, 0); /* fallback color */
