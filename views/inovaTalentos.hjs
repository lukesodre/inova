    font-family: customFont;
    
}

p{text-align: justify;}

#description-text h4{
    font-family: customFont;
    font-weight: 300;
    font-size: 2em;
    border-color: white;
    border-style: solid;
    border-width: thin;
    /*text-transform: none;*/
    background: rgb(0, 0, 0); /* fallback color */
    background: rgba(0, 0, 0, 0.7);
    padding: 10px; 
}
#description-text h3{
    font-family: customFont;
    font-weight: 300;
    font-size: 3em;
    border-color: white;
    border-style: solid;
    border-width: thin;
    /*text-transform: none;*/
    background: rgb(0, 0, 0); /* fallback color */
    background: rgba(0, 0, 0, 0.7);
    padding: 10px; 
}

.legend span{

        /*font-family: 'Gotham SSm A', 'Gotham SSm B',"Helvetica Neue", Helvetica, Arial, sans-serif;*/
    font-family: customFont;
    font-weight: 300;
    font-size: 3em;
    border-color: white;
    border-style: solid;
    border-width: thin;
    text-transform: uppercase;
    background: rgb(0, 0, 0); /* fallback color */
    background: rgba(0, 0, 0, 0.7);
    padding: 10px; 
    /*line-height: 200px;    */
}

.videoWrapper {
    position: relative;
    padding-bottom: 56.25%; /* 16:9 */
    padding-top: 25px;
    height: 0;
}
.videoWrapper iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}




.fonte-grande {
    font-size: 1.4em;
}

@media screen and (max-width: 720px) {

