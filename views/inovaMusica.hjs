<body id="page-top" class="index">

 {{> header}}



<section id="videosRecomendados">
    <div class="container">
        <h2 class="section-heading heading-h2">#INOVA Player</h2>
        <div class="inova-player">
        <div class="row" id="player-header">
            <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4">
                <img id="player-foto" src="images/rotating_card_profile.png" alt="">
            </div>
            <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8" id="player-header-musica">
                <div class="audio-player">
                    <h2 data-bind="text: $root.musicaSelecionada().nome"></h2>
                    <audio id="audio-player"  data-bind="attr:{src: $root.musicaSelecionada().link}" type="audio/mp3" controls="controls"></audio>
                </div><!-- @end .audio-player -->
                <br>
                <div class="player-header-buttons">
                    <a href="" class="btn btn-xl btn-default">Download Mp3</a>       
                    <a href="" class="btn btn-xl btn-default">Letra</a>             
                    <a href="" class="btn btn-xl btn-default">Cifra</a>  
                </div>     
            </div>
        </div> 

        <div class="row" id="player-bottom">
            <div class="col-xs-4 col-sm-4 col-md-4 col-lg-4" id="player-bottom-musicas">
                <br>
                <h2>Selecione uma Musica</h2>
                <table class="table table-striped" data-bind="foreach: musicas">
                    <tr ><td id="player-lista-cell"><a href="#" style="display:block;" data-bind="text: nome, click: $root.musicaSelecionada" ></a></td></tr>
                </table>   
            </div>
            <div class="col-xs-8 col-sm-8 col-md-8 col-lg-8" id="player-bottom-letra">
     
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <span data-bind="html: $root.musicaSelecionada().letra "></span>
                    </div>
                
                </div>
            </div> 
        </div>
    </div>
    

</section>




