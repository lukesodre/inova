<script type="text/javascript">
jQuery.fn.visible = function() {
    return this.css('display', 'initial');
};

jQuery.fn.invisible = function() {
    return this.css('display', 'none');
};

    
    $('#cifra').invisible();
    $('#btnCifra').click(function () {
        $('#cifra').visible();
        $('#letra').invisible();
        return false;
    })

    $('#btnLetra').click(function () {
        $('#cifra').invisible();
        $('#letra').visible();
        return false;
    })
</script>
