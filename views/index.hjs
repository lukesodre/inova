}

.item.w2 {
    width: 40%;
}

.imgAcampa {
    width: 100%;

}


.grid-sizer { width: 40.3%}

.container-mod {
      display: block;
  margin-left: 11%;
  margin-right: auto;
  width: 90%;
}
.item img { width: 100% }

.legend {
    /*opacity: 0.5;*/
    top:45%;
    /*left: 1%;*/
    color: white;
    /*position: absolute;*/
        left: 0;
    position:absolute;
    text-align:center;
    width: 100%


}
@font-face {
    font-family: customFont;
    src: url(font/font2.ttf);
}

.section-heading{
    font-family: customFont;
    
}

#description-text h4{
    font-family: customFont;
    font-weight: 300;
    font-size: 2em;
    border-color: white;
    border-style: solid;
    border-width: thin;
    /*text-transform: none;*/
    background: rgb(0, 0, 0); /* fallback color */
    background: rgba(0, 0, 0, 0.7);
    padding: 10px; 
}
#description-text h3{
    font-family: customFont;
    font-weight: 300;
    font-size: 3em;
    border-color: white;
    border-style: solid;
    border-width: thin;
    /*text-transform: none;*/
    background: rgb(0, 0, 0); /* fallback color */
    background: rgba(0, 0, 0, 0.7);
    padding: 10px; 
}

.legend span{

        /*font-family: 'Gotham SSm A', 'Gotham SSm B',"Helvetica Neue", Helvetica, Arial, sans-serif;*/
    font-family: customFont;
    font-weight: 300;
    font-size: 3em;
    border-color: white;
    border-style: solid;
    border-width: thin;
    text-transform: uppercase;
    background: rgb(0, 0, 0); /* fallback color */
    background: rgba(0, 0, 0, 0.7);
    padding: 10px; 
    /*line-height: 200px;    */
}

.videoWrapper {
    position: relative;
    padding-bottom: 56.25%; /* 16:9 */
    padding-top: 25px;
    height: 0;
}
.videoWrapper iframe {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.item-celula{
    width: 13%;
}

@media screen and (max-width: 720px) {

    .legend span{font-size: 2.5em;}
  /* 10 columns for larger screens */
    .item { width: 90%;}
    .item.w2 { width: 90%;}
    .grid-sizer { width: 100%; }
}
@media screen and (max-width: 1024px) {

