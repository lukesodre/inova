<body id="page-top" class="index">
   
   {{> header}}

    <!-- #INOVA_IGREJA Section -->
    <section id="inovaIgreja" >
        <div class="container-mod" >
            <div class="row">
                <div class="col-lg-12 ">
                    
                    <h2 class="section-heading heading-h2">#INOVA IGREJA</h2>
                    

                    <!-- <h3 class="section-subheading text-muted">Nova Dimensao de Vidas</h3> -->
                </div>
            </div>
            <!-- <div class="container2" class="js-masonry"  data-masonry-options='{ "itemSelector": ".item", "columnWidth": "200" }'> -->
            <!-- <div  id="container2" class="js-masonry" > -->
            <div id="container2"  class="js-masonry" >
                <div class="grid-sizer"></div>
                <div class="gutter-sizer"></div>
                <div id="inovaTalentos" class="item">
                    <a href="{{ links.linkInovaTalentos }}">
                        <img  src="img/inova_igreja/talentos2.jpg"  alt="Copyright Creative Commons Author: Kevin N. Murphy">      
                            <div class="legend">
                            <span>#INOVA Talentos</span>
                        </div>
                    </a>
                </div>
                <div  id="portalDaCelula" class="item w2">
                	<a href="/inovaCelula"><img   src="img/inova_igreja/celula.jpg"  alt=""> </a>     
                    <div  class="legend" >
                        <span >Portal da Celula</span>
                    </div>
                </div>         
                <div class="item">
                    <img  src="img/inova_igreja/musica.jpg"  alt="">      
                    <div class="legend">
