                    <!-- <h3 class="section-subheading text-muted">Nova Dimensao de Vidas</h3> -->
                </div>
            </div>
            <!-- <div class="container2" class="js-masonry"  data-masonry-options='{ "itemSelector": ".item", "columnWidth": "200" }'> -->
            <!-- <div  id="container2" class="js-masonry" > -->
            <div id="container3"  class="js-masonry">
                <div class="grid-sizer"></div>
                <div class="gutter-sizer"></div>
                <div class="item w2">
                    <img  src="img/inova_comunidade/violao.jpg"  alt="">      
                    <div class="legend" >
                        <span>Aulas de Musica</span>
                    </div>
                </div>         
                <div class="item">
                    <a href="/inovaBilingue"><img  src="img/inova_comunidade/alunos.jpg"  alt="">     
                        <div class="legend">
                            <span>Aulas de Ingles</span>
                        </div>
                    </a> 
                </div>
                <div class="item">
                    <img  src="img/inova_comunidade/emprego.jpg"  alt="">      
                    <div class="legend">
                        <span>Feira de empregos</span>
                    </div>
                </div>
            </div>
        </div>
